{
  "name": "layui",
  "version": "2.11.6",
  "description": "Classic modular Front-End UI library",
  "keywords": [
    "layui",
    "components",
    "front-end",
    "framework",
    "library",
    "ui",
    "web"
  ],
  "homepage": "https://layui.dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/layui/layui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/layui/layui.git"
  },
  "type": "module",
  "module": "./dist/layui.esm.js",
  "main": "dist/layui.js",
  "sideEffects": [
    "./dist/css/*"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:legacy": "gulp",
    "dev": "rollup -c -w",
    "release": "npm run build && npm rum release-zip",
    "release-zip": "gulp release"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "del": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.9",
    "gulp-replace": "^1.1.4",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.1.0",
    "minimist": "^1.2.8",
    "postcss-url": "^10.1.3",
    "rollup": "^4.50.0",
    "rollup-plugin-css-bundle": "^1.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-styles": "^4.0.0"
  },
  "dependencies": {
    "jquery": "^3.7.1"
  }
